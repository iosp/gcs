<UserControl x:Class="GCSArduino.View.RightCommandsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <Button Style="{DynamicResource ButtonWhiteStyle}" Content="Write" Command="{Binding WriteMissionCommand}"></Button>
        <Button Style="{DynamicResource ButtonWhiteStyle}" Content="Read" Command="{Binding ReadMissionCommand}"></Button>
        <Button Style="{DynamicResource ButtonWhiteStyle}" Content="Load" Command="{Binding LoadMissionCommand}"></Button>
        <Button Style="{DynamicResource ButtonWhiteStyle}" Content="Save" Command="{Binding SaveMissionCommand}"></Button>
    </StackPanel>
</UserControl>
